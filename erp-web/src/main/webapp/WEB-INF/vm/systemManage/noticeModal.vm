<div class="popup-basic popup-lg mfp-with-anim ">
    <div class="panel">
        <div class="panel-heading">
            <span class="panel-title"><i class="fa fa-pencil"></i>系统更新公告</span>
        </div>
        <div class="panel-body p25">
            <div class="section row">
                <script type="text/html" id="noticeDetailTpl">
                    {{#noticeObj}}
                    <div class="row mb20">
                        <div class="col-md-9 fs16 fw700 pl10">{{title}}</div>
                        <div class="col-md-3">{{#clockTime}}{{createTime}}{{/clockTime}}</div>
                    </div>
                    <div class="row">
                        <ul>
                            <li class="mb10">{{content}}</li>
                            <li class="mb10">{{remark}}</li>
                            <li class="mb10"><span>创建人</span>：{{createUserRealName}}</li>
                            <li class="mb10"><span>修改人</span>：{{updateUserRealName}}</li>
                            <li class="mb10"><span>修改时间</span>：{{#clockTime}}{{updateTime}}{{/clockTime}}</li>
                        </ul>
                    </div>
                    {{/noticeObj}}
                </script>
                <div class="col-md-12" id="noticeDetail"></div>
            </div>
        </div>
    </div>
</div>