<div id="auditPassModal" class="popup-basic admin-form mfp-with-anim popup-lg">
    <div class="panel">
        <div class="panel-heading">
            <span class="panel-title"><i class="fa fa-pencil"></i>更新任务执行者信息</span>
        </div>
        <form method="post" id="editPerformerForm">
            <div class="panel-body p25">
                <div class="section row">
                    <div class="col-md-6">
                        <label for="retryCount" class="field-label">重试次数</label>
                        <label for="retryCount" class="field prepend-icon">
                            <input type="text" name="retryCount" id="retryCount" class="gui-input" placeholder="重试次数">
                            <label for="retryCount" class="field-icon"><i class="fa fa-tag"></i></label>
                        </label>
                    </div>
                    <div class="col-md-6">
                        <label for="retryInterval" class="field-label">重试间隔</label>
                        <label for="retryInterval" class="field prepend-icon">
                            <input type="text" name="retryInterval" id="retryInterval" class="gui-input" placeholder="重试间隔">
                            <label for="retryInterval" class="field-icon"><i class="fa fa-tag"></i></label>
                        </label>
                    </div>
                </div>
                <div class="section row">
                    <div class="col-md-6">
                        <label for="requestUrl" class="field-label">请求的url</label>
                        <label for="requestUrl" class="field prepend-icon">
                            <input type="text" name="requestUrl" id="requestUrl" class="gui-input" placeholder="请求的url">
                            <label for="requestUrl" class="field-icon"><i class="fa fa-tag"></i></label>
                        </label>
                    </div>
                    <div class="col-md-6">
                        <label for="jobCallbackClassName" class="field-label">异步回调的完整类名</label>
                        <label for="jobCallbackClassName" class="field prepend-icon">
                            <input type="text" name="jobCallbackClassName" id="jobCallbackClassName" class="gui-input" placeholder="异步回调的完整类名">
                            <label for="jobCallbackClassName" class="field-icon"><i class="fa fa-tag"></i></label>
                        </label>
                    </div>
                </div>
                <div class="section row">
                    <div class="col-md-6">
                        <label class="field-label">异步标志</label>
                        <div class="field">
                            <div class="radio-custom radio-primary pull-left mr10">
                                <input type="radio" id="radio-synchronization" class="radios synchronization" name="radio-asyn">
                                <label for="radio-synchronization">同步</label>
                            </div>
                            <div class="radio-custom pull-left mb5">
                                <input type="radio" id="radio-asychronous" class="radios asychronous" name="radio-asyn">
                                <label for="radio-asychronous">异步</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="section row">
                    <div class="col-md-12">
                        <div class="section-divider mv40" id="spy3">
                            <span>请求的json字符串</span>
                        </div>
                        <div class="section mn">
                            <label for="requestBody" class="field prepend-icon">
                                <textarea class="gui-textarea" id="requestBody" name="requestBody" placeholder="请求的json字符串" style="height: 170px;"></textarea>
                                <label for="requestBody" class="field-icon"><i class="fa fa-comments"></i></label>
                            </label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel-footer text-right">
                <button type="submit" class="button btn-primary">确定</button>
                <button type="button" class="button btn-default cancelButton">取消</button>
            </div>
        </form>
    </div>
</div>