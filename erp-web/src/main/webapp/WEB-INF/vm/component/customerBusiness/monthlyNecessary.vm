                                            <input type="text" name="rentTimeLength-{{skuId}}-{{rowKey}}" value="{{rentTimeLength}}" class="rentTimeLength form-control required checkInteger pl10" placeholder="租赁时长" />
                                        </label>
                                    </div>
                                </td>
                                <td class="totalPrice">￥<span class="num">{{totalRowCurrentSkuPrice}}</span></td>
                                <td class="text-right">
                                    <div class="btn-group text-right">
                                        <a href="#" type="button" class="btn btn-default btn-xs deleteSKUButton" data-skuid="{{skuId}}" data-productid="{{productId}}">删除</a>
                                    </div>
                                </td>
                            </tr>
                            {{/listData}}
                            {{^listData}}
                            <tr>
                                <td valign="top" colspan="11" class="text-center">没有选择商品</td>
                            </tr>
                            {{/listData}}
                            <tr>
                                <td colspan="9" class="text-right text-danger" style="padding-right:0; margin-right:-11px;">首期所需设备总价值：</td>
                                <td colspan="2" class="text-danger">
                                    <div class="text-danger" id="firstListTotalPriceBox"></div>
                                    <input type="hidden" name="firstListTotalPrice" id="firstListTotalPrice" />
                                </td>
                            </tr>
                            </tbody>
                        </script>
                        <table class="table admin-form theme-warning tc-checkbox-1 fs13" id="dataListTable"></table>
                    </div>
