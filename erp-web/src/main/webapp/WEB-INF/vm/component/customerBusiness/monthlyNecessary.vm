                                <th>品牌</th>
                                <th>商品参数（SKU）</th>
                                <th>商品保值价</th>
                                <th>数量</th>
                                <th>类型</th>
                                <th>期限</th>
                                <th>商品总保值价</th>
                                <th class="text-right">操作</th>
                            </tr>
                            </thead>
                            <tbody>
                            {{#listData}}
                            <tr class="productRow skuRow" productRowKey="{{productId}}">
                                <td>
                                    <label class="option block mn">
                                        <input type="checkbox" name="checkItem" value="{{productId}}" data-skuid="{{skuId}}">
                                        <span class="checkbox mn"></span>
                                    </label>
                                </td>
                                <td>
                                    {{#isNewValueBadge}}{{isNewIntValue}}{{/isNewValueBadge}}
                                    {{productName}}
                                </td>
                                <td>{{#productInfo}}{{categoryName}}{{/productInfo}}</td>
                                <td>{{#productInfo}}{{brandName}}{{/productInfo}}</td>
                                <td><div style="max-width:300px;">{{propertiesToStr}}</div></td>
                                <td>￥<span class="unitPrice">{{currentSkuPrice}}</span>
                                    <input type="hidden" name="unitPrice" value="{{currentSkuPrice}}" />
                                </td>
                                <td>
                                    <input type="hidden" name="isNewProduct" value="{{isNewIntValue}}" class="isNewProduct" />
                                    <input type="hidden" name="productId" value="{{productId}}" class="productId" />
                                    <input type="hidden" name="skuId" value="{{skuId}}" class="productSkuId" />
                                    <label class="field prepend-icon" style="max-width:160px">
                                        <input type="text" name="rentCount-{{skuId}}-{{rowKey}}" value="{{rentCount}}" class="rentCount form-control required checkInteger pl10" placeholder="输入租赁数量" aria-required="true" aria-invalid="false"/>
                                    </label>
                                </td>
                                <td>
                                    <label class="field select">
                                        <select name="rentType-{{skuId}}-{{rowKey}}" class="rentType required" style="min-width: 130px;">
                                            <option value="">选择租赁类型</option>
                                            {{#rentTypeList}}
                                            <option value="{{num}}" {{#selected}}selected{{/selected}}>{{value}}</option>
                                            {{/rentTypeList}}
                                        </select>
                                        <i class="arrow"></i>
                                    </label>
                                </td>
