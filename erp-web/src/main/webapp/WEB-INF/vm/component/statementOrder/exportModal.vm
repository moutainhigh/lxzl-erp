<div id="submitAuditModal" class="popup-basic admin-form mfp-with-anim "> ##mfp-hide
    <div class="panel">
        <div class="panel-heading">
            <span class="panel-title"><i class="fa fa-pencil"></i>财务导出</span>
        </div>
        <form method="post" id="chooseForm" description="财务导出">
            <div class="panel-body p25">
                <div class="form-group field">
                    <label for="startAt" class="field-label text-right col-md-4">预计支付时间</label>
                    <div class="col-md-8">
                        <input type="hidden" name="" id="startTime" />
                        <input type="hidden" name="" id="endTime" />
                        <div class="input-group date" id="createTimePicker" style="height:40px;">
                            <input type="text" name="createTimePicker" id="createTimePickerInput" class="form-control bradiusL"  style="height:40px;" autocomplete="off" placeholder="选择预计支付时间">
                            <span class="input-group-addon cursor input-sm bradiusR"><i class="fa fa-calendar"></i></span>
                        </div>
                    </div>
                </div>
            </div>
        </form>

        <form action="" method="post" id="exportForm" target="exportFrame">
            <div class="panel-body pn">
                <input type="hidden" name="pageNo" id="pageNo" value="1" />
                <input type="hidden" name="pageSize" id="pageSize" value="100000000"/>
                <input type="hidden" name="createStartTime" id="createStartTime" />
                <input type="hidden" name="createEndTime" id="createEndTime" />
            </div>
            <div class="panel-footer text-right">
                <button type="submit" class="button btn-primary exportBtn">确定</button>
                <button type="button" class="button btn-default cancelButton">取消</button>
            </div>
        </form>
        <iframe id="exportFrame" name="exportFrame" src="about:blank" style="display:none;"></iframe>
    </div>
</div>