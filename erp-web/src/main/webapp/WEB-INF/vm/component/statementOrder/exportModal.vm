<div id="submitAuditModal" class="popup-basic admin-form mfp-with-anim "> ##mfp-hide
    <div class="panel">
        <div class="panel-heading">
            <span class="panel-title"><i class="fa fa-pencil"></i>财务导出</span>
        </div>
        <form action="${ctx}{{menuUrl}}exportExcel/exportPageBankSlipDetail" method="post" id="exportForm" target="exportFrame">
            <div class="panel-body p25">
                <div class="form-group field">
                    <label for="startAt" class="field-label text-right col-md-4">任务开始时间</label>
                    <div class="col-md-8">
                        <div class="input-group date" id="createTimePicker">
                            <input id="rentStartTime" name="rentStartTime" class="form-control" type="text" autocomplete="off"  placeholder="请选择时间" readonly />
                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel-footer text-right">
                <button type="submit" class="button btn-primary exportBtn">确定</button>
                <button type="button" class="button btn-default cancelButton">取消</button>
            </div>
        </form>
        <iframe id="exportFrame" name="exportFrame" src="about:blank" style="display:none;"></iframe>
    </div>
</div>