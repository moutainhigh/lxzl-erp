<div id="inputEndInfoModal" class="popup-basic popup-lg admin-form mfp-with-anim "> ##mfp-hide
    <div class="panel">
        <div class="panel-heading">
            <span class="panel-title"><i class="fa fa-pencil"></i><span class="modalTitle"></span></span>
        </div>
        <form method="post" class="inputForm">
            <div class="panel-body p25">
                <div class="section row">
                    <div class="col-md-12">
##                       <div class="col-md-6">
##                           <label for="statementCorrectAmount" class="field-label">冲正金额</label>
