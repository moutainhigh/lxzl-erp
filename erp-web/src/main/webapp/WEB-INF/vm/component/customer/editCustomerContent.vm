<div class="panel-body p25">
    <div class="section row">
        <div class="col-md-3">
            <label for="realName" class="field-label"><em class="text-danger">*</em>真实姓名</label>
            <label for="realName" class="field prepend-icon">
                <input type="text" name="realName" id="realName" class="gui-input required letterOrChinese" placeholder="请输入真实姓名..." maxlength="20">
                <label for="lastname" class="field-icon"><i class="fa fa-user"></i>
                </label>
            </label>
        </div>
        <div class="col-md-3">
            <label for="personNo" class="field-label"><em class="text-danger">*</em>身份证号</label>
            <label for="personNo" class="field prepend-icon">
                <input type="text" name="personNo" id="personNo" class="gui-input required checkIDCard" placeholder="请输入身份证号..." />
                <label for="personNo" class="field-icon"><i class="fa fa-user"></i>
                </label>
            </label>
        </div>
        <div class="col-md-3">
            <label for="email" class="field-label">邮箱</label>
            <label for="email" class="field prepend-icon">
                <input type="text" name="email" id="email" class="gui-input email" placeholder="请输入电子邮箱...">
                <label for="email" class="field-icon"><i class="fa fa-envelope"></i>
                </label>
            </label>
        </div>
        <div class="col-md-3">
            <label for="phone" class="field-label"><em class="text-danger">*</em>手机号</label>
            <label for="phone" class="field prepend-icon">
                <input type="text" name="phone" id="phone" class="gui-input required mobilephone" placeholder="请输入手机号...">
                <label for="website" class="field-icon"><i class="fa fa-mobile"></i>
                </label>
            </label>
        </div>
    </div>

    <div class="section row">
        <div class="col-md-3">
            <label for="customerOrigin" class="field-label"><em class="text-danger">*</em>业务员</label>
            <div class="smart-widget sm-right smr-50">
                <label class="field">
                    <input type="text" name="ownerName" id="ownerName" class="gui-input" readonly placeholder=" 请选择业务员" />
                    <input type="hidden" name="owner" id="owner" class="gui-input" />
                </label>
                <button type="button" class="button btn-primary bradiusN" id="deleteOwnerName" style="right:50px;"> <i class="fa fa-trash-o"></i></button>
                <button type="button" class="button btn-primary bradiusN" id="chooseOwnerName"> <i class="fa fa-search"></i>
                </button>
            </div>
        </div>
        <div class="col-md-3">
            <label for="customerOrigin" class="field-label">联合开发人</label>
            <div class="smart-widget sm-right smr-50">
                <label class="field">
                    <input type="text" name="unionUserName" id="unionUserName" class="gui-input" readonly />
                    <input type="hidden" name="unionUser" id="unionUser" class="gui-input" />
                </label>
                <button type="button" class="button btn-primary bradiusN" id="deleteUnionUserName" style="right:50px;"> <i class="fa fa-trash-o"></i></button>
                <button type="button" class="button btn-primary bradiusN" id="chooseUnionUserName"> <i class="fa fa-search"></i></button>
            </div>
        </div>
        <div class="col-md-3">
            <label for="firstApplyAmount" class="field-label"><em class="text-danger">*</em>首期申请额度</label>
            <label for="firstApplyAmount" class="field prepend-icon">
                <input type="text" name="firstApplyAmount" id="firstApplyAmount" class="gui-input required moneyNumber" />
                <label for="firstApplyAmount" class="field-icon"><i class="fa fa-jpy"></i></label>
            </label>
        </div>
        <div class="col-md-3">
            <label for="remark" class="field-label"><em class="text-danger">*</em>后期申请额度</label>
            <label for="laterApplyAmount" class="field prepend-icon">
                <input type="text" name="laterApplyAmount" id="laterApplyAmount" class="gui-input required moneyNumber" />
                <label for="laterApplyAmount" class="field-icon"><i class="fa fa-jpy"></i></label>
            </label>
        </div>
    </div>
    <div class="section row">
        <div class="col-md-3">
            <label for="statementDate" class="field-label"><em class="text-danger">*</em>结算日期</label>
            <label class="field select">
                <select id="statementDate" name="statementDate" class="required"></select>
                <i class="arrow"></i>
            </label>
        </div>
    </div>

    <div class="section row">
        <div class="col-md-6">
            <div class="section-divider mv40" id="spy2">
                <span>个人住址信息</span>
            </div>
            <div class="section row">
                <div class="col-md-6">
                    <div class="section">
                        <label class="field select">
                            <select id="province" name="province" class="required">
                                <option value="">请选择</option>
                            </select>
                            <i class="arrow"></i>
                        </label>
                    </div>
                    <div class="section">
                        <label class="field select">
                            <select id="city" name="city" class="required">
                                <option value="">请选择</option>
                            </select>
                            <i class="arrow"></i>
                        </label>
                    </div>
                    <div class="section">
                        <label class="field select">
                            <select id="district" name="district" class="required">
                                <option value="">请选择</option>
                            </select>
                            <i class="arrow"></i>
                        </label>
                    </div>
                </div>
                <div class="col-md-6">
                    <label for="address" class="field prepend-icon">
                        <textarea class="gui-textarea required" id="address" name="address" style="height: 170px;" placeholder="输入详细地址（街道信息）"></textarea>
                        <label for="address" class="field-icon"><i class="fa fa-map-marker"></i></label>
                    </label>
                </div>
            </div>
        </div>
        <div class="col-md-3">
            <div class="section-divider mv40" id="spy2">
                <span>紧急联系人信息</span>
            </div>
            <div class="section">
                <label for="connectRealName" class="field-label">紧急联系人姓名</label>
                <label for="connectRealName" class="field prepend-icon">
                    <input type="text" name="connectRealName" id="connectRealName" class="gui-input required letterOrChinese" placeholder="请输入紧急联系人姓名...">
                    <label for="connectRealName" class="field-icon"><i class="fa fa-user"></i>
                    </label>
                </label>
            </div>
            <div class="section">
                <label for="connectPhone" class="field-label">紧急联系人手机号</label>
                <label for="connectPhone" class="field prepend-icon">
                    <input type="text" name="connectPhone" id="connectPhone" class="gui-input required mobilephone" placeholder="请输入紧急联系人手机号...">
                    <label for="connectPhone" class="field-icon"><i class="fa fa-mobile"></i>
                    </label>
                </label>
            </div>
        </div>
        <div class="col-md-3">
            <div class="section-divider mv40" id="spy2">
                <span>备注</span>
            </div>
            <div class="section">
                <label for="remark" class="field prepend-icon">
                    <textarea class="gui-textarea" id="remark" name="remark" placeholder="请输入备注" style="height: 170px;"></textarea>
                    <label for="remark" class="field-icon"><i class="fa fa-comments"></i>
                    </label>
                </label>
            </div>
        </div>
    </div>
</div>
<div class="panel-footer text-right">
    <button type="submit" class="button btn-primary">确定</button>
    <button type="button" class="button btn-default goBack">返回</button>
</div>