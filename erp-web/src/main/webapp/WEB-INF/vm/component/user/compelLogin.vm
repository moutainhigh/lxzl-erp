<div id="compelLoginModal" class="popup-basic admin-form mfp-with-anim">
    <div class="panel">
        <div class="panel-heading">
            <span class="panel-title"><i class="fa fa-pencil"></i>强制登录</span>
        </div>

        <form method="post" id="compelLoginForm"  description="强制登录">
            <div class="panel-body p25">
                <div class="section row">
                    <div class="col-md-12">
                        
                        <label for="userName" class="field prepend-icon">
                            <input type="text" name="userName" id="userName" class="gui-input required" autocomplete="new-password" placeholder="输入用户名...">
                            <label for="userName" class="field-icon"><i class="fa fa-lock"></i>
                            </label>
                        </label>
                    </div>
                </div>

                <div class="section row">
                    <div class="col-md-12">
                        <input type="password" style="width: 1px; height: 1px; position: absolute; border: 0px; padding: 0px;">
                        <label for="password" class="field prepend-icon">
                            <input type="password" name="password" id="password" class="gui-input required" placeholder="输入管理员密码" maxlength="18">
                            <label for="password" class="field-icon"><i class="fa fa-unlock-alt"></i>
                            </label>
                        </label>
                    </div>
                </div>
            </div>

            <div class="panel-footer text-right">
                <button type="submit" class="button btn-primary">登录</button>
            </div>
        </form>
    </div>
</div>