<div id="inputEndInfoModal" class="popup-basic admin-form mfp-with-anim "> ##mfp-hide
    <div class="panel">
        <div class="panel-heading">
            <span class="panel-title"><i class="fa fa-pencil"></i><span class="modalTitle"></span></span>
        </div>
        <form method="post" class="inputForm">
            <div class="panel-body p25">
                <div class="section row">
                    <div class="col-md-12">
                        <label for="serviceCost" class="field-label">服务费</label>
                        <label for="serviceCost" class="field prepend-icon">
                            <input type="text" name="serviceCost" id="serviceCost" class="gui-input required moneyNumber" maxlength="10">
                            <label for="serviceCost" class="field-icon"><i class="fa fa-tag"></i>
                            </label>
                        </label>
                    </div>
                </div>
                <div class="section row">
                    <div class="col-md-12">
                        <label for="isDamage" class="field-label">是否损坏</label>
                        <label class="field select">
                            <select id="isDamage" name="isDamage" class="required">
                                <option value="">请选择</option>
                                <option value="0">否</option>
                                <option value="1">是</option>
                            </select>
                            <i class="arrow"></i>
                        </label>
                    </div>
                </div>
                <div class="section row">
                    <div class="col-md-12">
                        <label for="damageCost" class="field-label">损坏加收费</label>
                        <label for="damageCost" class="field prepend-icon">
                            <input type="text" name="damageCost" id="damageCost" class="gui-input required moneyNumber" maxlength="10">
                            <label for="damageCost" class="field-icon"><i class="fa fa-tag"></i>
                            </label>
                        </label>
                    </div>
                </div>
                <div class="section row">
                    <div class="col-md-12">
                        <label for="isDamage" class="field-label">是否损坏</label>
                        <label class="field select">
                            <select id="changeMode" name="changeMode" class="required"></select>
                            <i class="arrow"></i>
                        </label>
                    </div>
                </div>
                <div class="section row">
                    <div class="col-md-12">
                        <label class="field-label">换货时间</label>
                        <div for="rentStartTime" id="rentStartTimePicker" class="field prepend-icon">
                            <input type="text" id="rentStartTime" name="rentStartTime" class="gui-input hasDatepicker required" placeholder="">
                            <label class="field-icon"><i class="fa fa-calendar-o"></i>
                            </label>
                        </div>
                    </div>
                </div>
                <div class="section row">
                    <div class="col-md-12">
                        <label for="remark" class="field-label">备注</label>
                        <label for="remark" class="field prepend-icon">
                            <textarea class="gui-textarea" id="remark" name="remark" placeholder="请输入备注"></textarea>
                            <label for="remark" class="field-icon"><i class="fa fa-comments"></i>
                            </label>
                        </label>
                    </div>
                </div>
            </div>
            <div class="panel-footer text-right">
                <button type="submit" class="button btn-primary">确定</button>
                <button type="button" class="button btn-default cancelButton"">取消</button>
            </div>
        </form>
    </div>
</div>