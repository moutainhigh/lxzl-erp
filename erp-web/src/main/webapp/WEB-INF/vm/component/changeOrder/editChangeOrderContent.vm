##                                    <address class="mn">
##                                        还没有添加换货地址
##                                    </address>
##                                </div>
##                            </div>
##                        </div>
                    </div>
                </div>

                <div class="form-group field">
                    <label class="col-md-2 control-label"><em class="text-danger">*</em>联系人</label>
                    <div class="col-md-10">
                        <div class="input-group" style="width:100%;">
                            <input id="ownerName" name="ownerName" bringFiled="ownerName" class="form-control required" type="text" placeholder="请输入联系人" />
                        </div>
                    </div>
                </div>

                <div class="form-group field">
                    <label class="col-md-2 control-label"><em class="text-danger">*</em>联系电话</label>
                    <div class="col-md-10">
                        <div class="input-group" style="width:100%;">
                            <input id="ownerName" name="ownerName" bringFiled="ownerName" class="form-control required" type="text" placeholder="请输入联系电话" />
                        </div>
                    </div>
                </div>

                <div class="form-group field">
                    <label class="col-md-2 control-label"><em class="text-danger">*</em>换货日期</label>
                    <div class="col-md-10">
                        <div class="input-group date" id="rentStartTimePicker">
                            <input id="rentStartTime" name="rentStartTime" class="form-control required" type="text" autocomplete="off"  placeholder="请选择换货日期" />
                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                        </div>
                    </div>
                </div>

                <div class="form-group field">
                    <label class="col-md-2 control-label"><em class="text-danger">*</em>换货方式</label>
                    <div class="col-md-10">
                        <div class="input-group" style="width: 100%;">
                            <select class="form-control required" name="changeMode"></select>
                        </div>
                    </div>
                </div>

                <div class="form-group field">
                    <label class="col-md-2 control-label"><em class="text-danger">*</em>换货原因</label>
                    <div class="col-md-10">
                        <div class="input-group" style="width: 100%;">
                            <select class="form-control required" name="changeReasonType" id="changeReasonType"></select>
                        </div>
                    </div>
                </div>

                <div class="form-group field">
                    <label class="col-md-2 control-label">备注</label>
                    <div class="col-md-10">
                        <textarea class="form-control" id="remark" rows="3"  name="remark"></textarea>
                    </div>
                </div>

                <div class="form-group field">
                    <label class="col-md-2 control-label">运费</label>
                    <div class="col-md-2" style="padding-right:0;">
                        <div class="input-group">
                            <input id="ownerName" name="ownerName" bringFiled="ownerName" class="form-control required" type="text" readonly placeholder="请输入运费" />
                        </div>
                    </div>
                    <label class="col-md-2 control-label">服务费</label>
                    <div class="col-md-2">
                        <div class="input-group" style="padding-right:0;">
                            <input id="ownerName" name="ownerName" bringFiled="ownerName" class="form-control required" type="text" readonly placeholder="请输入服务费" />
                        </div>
                    </div>
                    <label class="col-md-2 control-label">维修费</label>
                    <div class="col-md-2">
                        <div class="input-group">
                            <input id="ownerName" name="ownerName" bringFiled="ownerName" class="form-control required" type="text" readonly placeholder="请输入维修费" />
                        </div>
                    </div>
                </div>

                <div class="form-group field">
                    <label class="col-md-2 control-label">发货公司</label>
                    <div class="col-md-10">
                        <div id="deliveryCompany"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel-heading">
        <div class="row">
            <ul class="nav panel-tabs-border panel-tabs panel-tabs-left">
                <li class="active">
                    <a href="#purchaseOrderProductItems" data-toggle="tab">商品项（共<span id="orderItemProductCount">0</span>件）</a>
                </li>
            </ul>
        </div>
    </div>
    <div class="panel-body dark pn">
        <div class="tab-content pn br-n">
            <div id="purchaseOrderProductItems" class="tab-pane active">
                <div class="panel-menu p12 br-n br-b">
                    <div class="row">
                        <div class="pull-left ml10">
                            <div class="btn-group" id="actionCommonButtons">
                                <button type="button" class="btn btn-primary btn-sm" id="batchAddProduct" title="选择商品">
                                    <i class="fa fa-plus"></i> 选择商品
                                </button>
                                <button type="button" class="btn btn-primary btn-sm" id="batchDeleteProduct" title="删除商品">
                                    <i class="fa fa-trash-o"></i> 删除
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="table-responsive">
                    <script type="text/html" id="dataListTpl">
                        <thead>
                        <tr class="bg-light">
                            <th>
                                <label class="option block mn">
                                    <input type="checkbox" name="checkAll" value="">
                                    <span class="checkbox mn"></span>
                                </label>
                            </th>
                            <th>订单编号</th>
                            <th>商品编号</th>
                            <th>单价</th>
                            <th>在租数</th>
                            <th>更换商品信息</th>
                            <th>更换数量</th>
                            <th>更换商品单价</th>
                            <th>备注</th>
                            <th>操作</th>
                        </tr>
                        </thead>
                        <tbody>
                        {{#listData}}
                        <tr class="productRow skuRow" productRowKey="{{productId}}">
                            <td {{#destChangeProductInfo}}rowSpan="2" class="br-r"{{/destChangeProductInfo}}>
                                <label class="option block mn">
                                    <input type="checkbox" name="checkItem" value="{{productId}}" data-skuid="{{skuId}}">
                                    <span class="checkbox mn"></span>
                                </label>
                            </td>
                            <td>{{productName}}</td>
                            <td>{{#productInfo}}{{categoryName}}{{/productInfo}}</td>
                            <td>{{#productInfo}}{{brandName}}{{/productInfo}}</td>
                            <td>{{propertiesToStr}}</td>
                            <td></td>
                            <td>
                                <label class="field prepend-icon mw200">
                                    <input type="hidden" name="skuId" value="{{productId}}" class="productId" />
                                    <input type="hidden" name="skuId" value="{{skuId}}" class="productSkuId" />
                                    <input type="text" name="changeProductSkuCount-{{skuId}}" value="{{changeProductSkuCount}}" class="changeProductSkuCount form-control required checkInteger pl10" placeholder="输入换货数量" />
                                </label>
                            </td>
                            <td>
                                <div class="smart-widget sm-right smr-50 mw200">
                                    <label class="field prepend-icon">
                                        <input type="text" name="productUnitAmount-1719-79" value="" class="productUnitAmount form-control required gui-input" placeholder="支持四位小数">
                                        <label for="listPrice" class="field-icon"><i class="fa fa-jpy"></i></label>
                                    </label>
                                </div>
                            </td>
                            <td>{{propertiesToStr}}</td>
                            <td>
                                <div class="btn-group text-right">
                                    <a href="#" type="button" class="btn btn-default btn-xs chooseDestChangeProduct" data-skuid="{{skuId}}" data-productid="{{productId}}">选择更换商品</a>
                                    <a href="#" type="button" class="btn btn-default btn-xs deleteSKUButton" data-skuid="{{skuId}}" data-productid="{{productId}}">删除</a>
                                </div>
                            </td>
                        </tr>
                        {{/listData}}
                        {{^listData}}
                        <tr>
                            <td valign="top" colspan="10" class="text-center">没有选择商品</td>
                        </tr>
                        {{/listData}}
                        </tbody>
                    </script>
                    <table class="table admin-form theme-warning tc-checkbox-1 fs13" id="dataListTable"></table>
                </div>
            </div>
        </div>
    </div>
    <div class="panel-footer clearfix">
        <button class="btn btn-default pull-right ml15 goback">返回</button>
        <button class="btn btn-primary pull-right">保存</button>
    </div>
</div>