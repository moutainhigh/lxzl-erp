<div class="panel panel-primary panel-border top mb35">
    <div class="panel-heading">
        <span class="panel-title">基本信息</span>
    </div>
    <div class="form-horizontal panel-body bg-light dark">
        <div class="section row">
            <div class="col-md-1"></div>
            <div class="col-md-6">

                <div class="form-group field">
                    <label class="col-md-2 control-label">客户</label>
                    <div class="col-md-10">
                        <div class="input-group bringContainer">
                            <input type="hidden" id="buyerCustomerName" name="customerName" bringFiled="customerName" class="form-control required" />
                        </div>
                    </div>
                </div>

                <div class="form-group field">
                    <label class="col-md-2 control-label">换货地址</label>
                    <div class="col-md-10">
                        <script type="text/html" id="orderConsignInfoTpl">
                            {{#address}}
                            <p class="mn">{{provinceName}}{{cityName}}{{districtName}}{{address}}</p>
                            {{/address}}
                        </script>
                        <div id="orderConsignInfo"></div>
