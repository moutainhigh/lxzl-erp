<div id="changeOrderList" class="tab-pane tab-pane-order">
    <div class="table-responsive">
        <script type="text/html" id="changeOrderTpl">
            <thead>
            <tr class="bg-light">
                <th class="nowrap">换货单编号</th>
                <th class="nowrap">商品信息</th>
                <th class="nowrap">K3编号</th>
                <th class="nowrap">换货数量</th>
                <th class="nowrap">实际换货数量</th>
                <th class="nowrap">换货时间</th>
                <th class="nowrap">商品总价</th>
                <th class="nowrap">备注</th>
            </tr>
            </thead>
            <tbody>
            {{#dataSource}}
            {{#listData}}
            <tr>  
                <td class="nowrap"><a href="{{detailUrl}}" target="_blank">{{replaceOrderNo}}</a></td>
                <td class="nowrap">{{rentStartTime}}</td>
                <td class="nowrap">{{#clockShorTime}}{{expectReturnTime}}{{/clockShorTime}}</td>
                <td class="nowrap">{{#rentTypeByMonth}}{{statementDateValue}}{{/rentTypeByMonth}}</td>
                <td class="nowrap">{{totalProductCount}}</td>
                <td class="nowrap">{{totalProductAmount}}</td>
                <td class="nowrap">{{totalMaterialCount}}</td>
                <td class="nowrap">{{totalMaterialCount}}</td>
            </tr>
            {{/listData}}
            {{^listData}}
            <tr>
                <td valign="top" colspan="8" class="text-center">无换货单</td>
            </tr>
            {{/listData}}
            {{/dataSource}}
            </tbody>
        </script>
        <table class="table admin-form theme-warning tc-checkbox-1 fs13 br-b" id="changeOrderTable"></table>
    </div>
</div>