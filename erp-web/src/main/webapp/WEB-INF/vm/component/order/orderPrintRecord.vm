            <table class="table admin-form theme-warning tc-checkbox-1 fs13" id="printRecordTable"></table>
        </div>
    </div>
    <div class="row br-t" style="margin:0;">
        <div class="col-xs-12 col-md-12 text-right mt10 mb10">
