<div id="sectionSettlementModal" class="popup-basic popup-lg admin-form mfp-with-anim ">
    <div class="panel">
        <div class="panel-heading">
            <span class="panel-title"><i class="fa fa-pencil"></i><span class="modalTitle">库存查询</span></span>
        </div>
        <div class="panel-menu p25">
            <div class="row">
                <div class="col-lg-12">
                    <form method="post" class="inputForm">
                        <div class="section row">
                            <div class="col-md-6">
                                <label for="changeTimePicker" class="field-label">分段时间点</label>
                                <div>
                                    <div class="input-group date" id="changeTimePicker">
                                        <input type="hidden" name="statementDateChangeTime" id="statementDateChangeTime">
                                        <input type="text" readonly id="changeTimeInput" data-type="date" class="form-control" autocomplete="off" placeholder="选择时间" style="height:42px;">
                                        <span class="input-group-addon cursor"><i class="fa fa-calendar"></i></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <label for="changeType" class="field-label">尾段首月结算方式</label>
                                <label class="field select">
                                    <select id="changeType" name="changeType">
                                    </select>
                                    <i class="arrow"></i>
                                </label>
                            </div>
                        </div>
                        <div class="section row">
                            <div class="col-md-6">
                                <label for="beforeStatementDate" class="field-label">首段结算日</label>
                                <label class="field select">
                                    <select id="beforeStatementDate" name="beforeStatementDate" class="settlementSelect">
                                    </select>
                                    <i class="arrow"></i>
                                </label>
                            </div>
                            <div class="col-md-6">
                                <label for="afterStatementDate" class="field-label">尾段结算日</label>
                                <label class="field select">
                                    <select id="afterStatementDate" name="afterStatementDate" class="settlementSelect">
                                    </select>
                                    <i class="arrow"></i>
                                </label>
                            </div>
                        </div>
                        <hr class="short alt">
                        <div class="section row">
                            <div class="col-md-12">
                                <div class="pull-right mn mr15">
                                    <button type="button" id="confirmBtn" class="btn btn-primary btn-sm">查询</button>
                                    <button type="button" id="searchBtn" class="btn btn-default btn-sm">重置</button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <div class="panel-body p25 hide" id="stockSection">
            <div class="section row hide">
                <div class="table-responsive">
                    <script type="text/html" id="productStockTpl">
                        <thead>
                        <tr class="bg-light">
                            <th>仓库</th>
                            <th>需求库存是否满足</th>
                            <th>库存数量</th>
                        </tr>
                        </thead>
                        <tbody>
                        {{#listData}}
                        <tr class="productRow" productRowKey="{{productId}}">
                            <td>{{#productInfo}}{{categoryName}}{{/productInfo}}</td>
                            <td>{{#productInfo}}{{brandName}}{{/productInfo}}</td>
                            <td>{{propertiesToStr}}</td>
                        </tr>
                        {{/listData}}
                        {{^listData}}
                        <tr>
                            <td valign="top" colspan="3" class="text-center">无库存</td>
                        </tr>
                        {{/listData}}
                        </tbody>
                    </script>
                    <table class="table admin-form theme-warning tc-checkbox-1 fs13" id="productStockTable">
                        <thead>
                        <tr class="bg-light">
                            <th>仓库</th>
                            <th>需求库存是否满足</th>
                            <th>库存数量</th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr>
                            <td valign="top" colspan="3" class="text-center">无商品库存</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>    
        <div class="panel-footer text-right">
            <button type="button" id="cancelButton" class="button btn-default">关闭</button>
        </div>
    </div>
</div>