<!DOCTYPE html>
<html>
<head>
    <title>爱租-运营管理系统-客户管理-客户列表</title>
    #parse("layout/header.vm")
    <link rel="stylesheet" type="text/css" href="${staticCommon}/vendor/plugins/datepicker/css/bootstrap-datetimepicker.min.css">
    <link rel="stylesheet" type="text/css" href="${staticCommon}/vendor/plugins/daterange/daterangepicker.css">
</head>
<body class="blank-page">
    #parse("layout/toolbox.vm")
<div id="main">
    #parse("layout/navbar.vm")
    #parse("layout/sidebar.vm")


    <section id="content_wrapper">
        #parse("layout/topbarDropmenu.vm")
        #parse("layout/topbar.vm")
        <section id="content" class="animated fadeIn">

            <div class="panel mb25 mt5">
                <form id="searchForm">
                    <div class="panel-heading">
                        <span class="panel-title">商品查询</span>
                    </div>
                    <div class="panel-body p25 pb5 admin-form">
                        <div class="section row">
                            <div class="col-md-3">
                                <label for="username" class="field-label">客户ID</label>
                                <label for="userId" class="field prepend-icon">
                                    <input type="text" name="userId" id="userId" class="gui-input number" />
                                    <label for="userId" class="field-icon"><i class="fa fa-tag"></i>
                                    </label>
                                </label>
                            </div>
                            <div class="col-md-3">
                                <label for="username" class="field-label">真实姓名</label>
                                <label for="realName" class="field prepend-icon">
                                    <input type="realName" name="realName" id="realName" class="gui-input" />
                                    <label for="realName" class="field-icon"><i class="fa fa-tag"></i>
                                    </label>
                                </label>
                            </div>
                            <div class="col-md-3">
                                <label for="username" class="field-label">激活</label>
                                <label class="field select">
                                    <select id="isActivated" name="isActivated">
                                        <option value="">所有</option>
                                        <option value="1">是</option>
                                        <option value="0">否</option>
                                    </select>
                                    <i class="arrow"></i>
                                </label>
                            </div>
                            <div class="col-md-3">
                                <label for="username" class="field-label">禁用</label>
                                <label class="field select">
                                    <select id="isDisabled" name="isDisabled">
                                        <option value="">所有</option>
                                        <option value="1">是</option>
                                        <option value="0">否</option>
                                    </select>
                                    <i class="arrow"></i>
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="panel-footer text-right admin-form">
                        <button type="submit" class="btn btn-primary"> 查询 </button>
                        <button type="reset" class="btn"> 重置 </button>
                    </div>
                </form>
            </div>


            <div class="panel">
                <div class="panel-menu p12 theme-primary">
                    <div class="row table-layout">
                        <div class="col-md-6">
                            <div class="btn-group">
                                <button type="button" class="btn btn-default light" id="btnRefresh"><i class="fa fa-refresh"></i>
                                </button>
                            #*<button type="button" class="btn btn-default btn-sm light"><i class="fa fa-trash"></i></button>*#
                            </div>
                        </div>
                        <div class="col-md-6 text-right">
                        </div>
                    </div>
                </div>
                <div class="panel-body pn">
                    <div class="table-responsive">
                        <table class="table admin-form theme-warning tc-checkbox-1 fs13">
                            <thead>
                            <tr class="bg-light">
                                #*<th class="text-center">
                                    <label class="option block mn">
                                        <input type="checkbox" name="orders" value="">
                                        <span class="checkbox mn"></span>
                                    </label>
                                </th>*#
                                <th class="text-center">客户ID</th>
                                <th>客户类型</th>
                                <th>客户名</th>
                                <th>真实姓名</th>
                                <th>邮箱</th>
                                <th>手机号</th>
                                <th>激活</th>
                                <th>禁用</th>
                                <th>注册时间</th>
                                <th>最后登录时间</th>
                                <th>最后登录IP</th>
                                <th class="text-right">操作</th>
                            </tr>
                            </thead>
                            <script type="text/html" id="dataListTpl">
                                {{#listData}}
                                <tr>
                                    #*<td class="text-center">
                                        <label class="option block mn">
                                            <input type="checkbox" name="mobileos" value="FR">
                                            <span class="checkbox mn"></span>
                                        </label>
                                    </td>*#
                                    <td>{{userId}}</td>
                                    <td>{{cutomerTypeValue}}</td>
                                    <td>{{userName}}</td>
                                    <td>{{realName}}</td>
                                    <td>{{email}}</td>
                                    <td>{{phone}}</td>
                                    <td>{{isActivatedStr}}</td>
                                    <td>{{isDisabledStr}}</td>
                                    <td>{{registerTimeFormat}}</td>
                                    <td>{{lastLoginTimeFormat}}</td>
                                    <td>{{lastLoginIp}}</td>
                                    <td class="text-right">
                                        {{#acitonButtons}}
                                        <div class="btn-group text-right">
                                            {{#acitons}}
                                            <a target="_blank" href="${ctx}{{menuUrl}}?customerId={{userId}}" class="btn btn-default btn-xs {{class}}" data-status="{{status}}" data-infoid="{{collegePersonInfoId}}">{{text}}</a>
                                            {{/acitons}}
                                        </div>
                                        {{/acitonButtons}}
                                    </td>
                                </tr>
                                {{/listData}}
                                {{^listData}}
                                <tr>
                                    <td valign="top" colspan="14">没有查询客戶</td>
                                </tr>
                                {{/listData}}
                            </script>
                            <tbody id="dataListTbody"></tbody>
                        </table>
                    </div>
                </div>
                <div class="panel-footer">
                    <div class="row table-layout">
                        <div class="col-xs-6 col-md-6 text-right">
                        </div>
                        #parse("component/pager.vm")
                    </div>
                </div>

            </div>
        </section>
    </section>

    #parse("layout/sidebarRight.vm")
</div>
    #parse("layout/footer.vm")
<script type="text/javascript" src="${staticCommon}/vendor/plugins/daterange/moment.min.js"></script>
<script type="text/javascript" src="${staticCommon}/vendor/plugins/daterange/daterangepicker.js"></script>
<script type="text/javascript">
    management_load_script('${staticManagement}/js','manage.customer.js');
</script>
<script type="text/javascript">
    //功能事件初始化
    Rental.init();
    Layout.init();
    CustomerManage.init();

    //UI效果初始化
    Core.init();
    Demo.init();
</script>

</body>
</html>