<div id="addBusinessCustomerModal" class="popup-basic popup-xl admin-form mfp-with-anim "> ##mfp-hide
    <div class="panel">
        <div class="panel-heading">
            <span class="panel-title"><i class="fa fa-pencil"></i>添加企业客户</span>
        </div>
        <form method="post" id="addBusinessCustomerForm" description="添加企业客户">
            <div class="panel-body p25">
                <div class="section row">
                    <div class="col-md-4">
                        <label for="companyName" class="field-label">公司名称</label>
                        <label for="companyName" class="field prepend-icon">
                            <input type="text" name="companyName" id="companyName" class="gui-input required" placeholder="请输入公司名称" />
                            <label for="companyName" class="field-icon"><i class="fa fa-user"></i>
                            </label>
                        </label>
                    </div>
                    <div class="col-md-4">
                        <label for="companyAbb" class="field-label">公司简称</label>
                        <label for="companyAbb" class="field prepend-icon">
                            <input type="text" name="companyAbb" id="companyAbb" class="gui-input required" placeholder="请输入公司简称" />
                            <label for="companyAbb" class="field-icon"><i class="fa fa-user"></i>
                            </label>
                        </label>
                    </div>
                    <div class="col-md-4">
                        <label for="businessLicenseNo" class="field-label">营业执照号</label>
                        <label for="businessLicenseNo" class="field prepend-icon">
                            <input type="text" name="businessLicenseNo" id="businessLicenseNo" class="gui-input required" placeholder="请输入营业执照号" />
                            <label for="businessLicenseNo" class="field-icon"><i class="fa fa-user"></i>
                            </label>
                        </label>
                    </div>
                </div>
                <div class="section row">
                    <div class="col-md-4">
                        <label for="legalPerson" class="field-label">法人姓名</label>
                        <label for="legalPerson" class="field prepend-icon">
                            <input type="text" name="legalPerson" id="legalPerson" class="gui-input required letterOrChinese" placeholder="请输入法人姓名" />
                            <label for="legalPerson" class="field-icon"><i class="fa fa-user"></i>
                            </label>
                        </label>
                    </div>
                    <div class="col-md-4">
                        <label for="legalPersonNo" class="field-label">法人身份证号</label>
                        <label for="legalPersonNo" class="field prepend-icon">
                            <input type="text" name="legalPersonNo" id="legalPersonNo" class="gui-input required" placeholder="请输入法人身份证号" />
                            <label for="legalPersonNo" class="field-icon"><i class="fa fa-user"></i>
                            </label>
                        </label>
                    </div>
                    <div class="col-md-4">
                        <label for="landline" class="field-label">座机电话</label>
                        <label for="landline" class="field prepend-icon">
                            <input type="text" name="landline" id="landline" class="gui-input required telephone" placeholder="请输入座机电话" />
                            <label for="landline" class="field-icon"><i class="fa fa-user"></i>
                            </label>
                        </label>
                    </div>
                </div>
                <div class="section row">
                    <div class="col-md-4">
                        <label for="connectRealName" class="field-label">联系人姓名</label>
                        <label for="connectRealName" class="field prepend-icon">
                            <input type="text" name="connectRealName" id="connectRealName" class="gui-input required letterOrChinese" placeholder="请输入联系人姓名" />
                            <label for="connectRealName" class="field-icon"><i class="fa fa-user"></i></label>
                        </label>
                    </div>
                    <div class="col-md-4">
                        <label for="connectPhone" class="field-label">联系人手机号</label>
                        <label for="connectPhone" class="field prepend-icon">
                            <input type="text" name="connectPhone" id="connectPhone" class="gui-input required mobilephone" placeholder="请输入真实姓名..." />
                            <label for="connectPhone" class="field-icon"><i class="fa fa-user"></i>
                            </label>
                        </label>
                    </div>
                    <div class="col-md-4">
                        <label for="isDisabled" class="field-label">禁用</label>
                        <label class="field select">
                            <select id="isDisabled" name="isDisabled">
                                <option value="0">否</option>
                                <option value="1">是</option>
                            </select>
                            <i class="arrow"></i>
                        </label>
                    </div>
                </div>
                <div class="section row">
                    <div class="col-md-4">
                        <label for="province" class="field-label">省份</label>
                        <label class="field select">
                            <select id="province" name="province" class="required">
                                <option value="">请选择</option>
                            </select>
                            <i class="arrow"></i>
                        </label>
                    </div>
                    <div class="col-md-4">
                        <label for="city" class="field-label">城市</label>
                        <label class="field select">
                            <select id="city" name="city" class="required">
                                <option value="">请选择</option>
                            </select>
                            <i class="arrow"></i>
                        </label>
                    </div>
                    <div class="col-md-4">
                        <label for="district" class="field-label">地区</label>
                        <label class="field select">
                            <select id="district" name="district" class="required">
                                <option value="">请选择</option>
                            </select>
                            <i class="arrow"></i>
                        </label>
                    </div>
                </div>
                <div class="section row">
                    <div class="col-md-12">
                        <label for="address" class="field-label">详细地址</label>
                        <label for="address" class="field prepend-icon">
                            <textarea class="gui-textarea" id="address" name="address" placeholder="请输入详细地址"></textarea>
                            <label for="address" class="field-icon"><i class="fa fa-map-marker"></i></label>
                        </label>
                    </div>
                </div>
                <div class="section row">
                    <div class="col-md-12">
                        <label for="remark" class="field-label">备注</label>
                        <label for="remark" class="field prepend-icon">
                            <textarea class="gui-textarea" id="remark" name="remark" placeholder="请输入备注"></textarea>
                            <label for="remark" class="field-icon"><i class="fa fa-comments"></i></label>
                        </label>
                    </div>
                </div>
            </div>
            <div class="panel-footer text-right">
                <button type="submit" class="button btn-primary">确定</button>
                <button type="button" class="button btn-default cancelButton">取消</button>
            </div>
        </form>
    </div>
</div>