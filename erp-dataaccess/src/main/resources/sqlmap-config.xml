        <mapper resource="sqlmap/erp_product_category_property.xml" />
        <mapper resource="sqlmap/erp_product_category_property_value.xml" />
        <mapper resource="sqlmap/erp_product_equipment.xml" />
        <mapper resource="sqlmap/erp_product_equipment_repair_record.xml" />
