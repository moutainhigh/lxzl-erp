        <mapper resource="sqlmap/erp_role_menu.xml" />
        <mapper resource="sqlmap/erp_sys_menu.xml" />
        <mapper resource="sqlmap/erp_user.xml" />
        <mapper resource="sqlmap/erp_user_role.xml" />
        <mapper resource="sqlmap/erp_materiel.xml" />
        <mapper resource="sqlmap/erp_equipment_materie.xml" />
