<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lxzl.erp.dataaccess.dao.mysql.userLoginLog.UserLoginLogMapper">

	<resultMap id="UserLoginLogDO" type="com.lxzl.erp.dataaccess.domain.userLoginLog.UserLoginLogDO">
		<id column="id" jdbcType="INTEGER" property="id" />
		<result column="user_name" jdbcType="VARCHAR" property="userName" />
		<result column="login_ip" jdbcType="VARCHAR" property="loginIp" />
		<result column="login_mac_address" jdbcType="VARCHAR" property="loginMacAddress" />
		<result column="remark" jdbcType="VARCHAR" property="remark" />
		<result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
	</resultMap>

	<sql id="column_List">
		eull.id,eull.user_name,eull.login_ip,eull.login_mac_address,eull.remark,eull.create_time
	</sql>

	<select id="findById" resultMap="UserLoginLogDO" parameterType="java.lang.Integer">
		SELECT <include refid="column_List"/> FROM erp_user_login_log eull 
		WHERE eull.id = #{id, jdbcType=INTEGER} AND eull.data_status = 1 
	</select>

	<select id="listCount" resultType="java.lang.Integer" parameterType="map">
		SELECT count(eull.id) FROM erp_user_login_log eull 
		<where>
			<if test="true">
				AND eull.data_status = 1
			</if>
		</where>
	</select>

	<select id="listPage" resultMap="UserLoginLogDO" parameterType="map">
		SELECT <include refid="column_List"/> FROM erp_user_login_log eull 
		<where>
			<if test="true">
				AND eull.data_status = 1
			</if>
		</where>
		LIMIT #{maps.start},#{maps.pageSize}
	</select>

	<sql id="set_column_sql">
		<set>
			<if test="userName != null">
				user_name = #{userName,jdbcType=VARCHAR},
			</if>
			<if test="loginIp != null">
				login_ip = #{loginIp,jdbcType=VARCHAR},
			</if>
			<if test="loginMacAddress != null">
				login_mac_address = #{loginMacAddress,jdbcType=VARCHAR},
			</if>
			<if test="remark != null">
				remark = #{remark,jdbcType=VARCHAR},
			</if>
			<if test="createTime != null">
				create_time = #{createTime,jdbcType=TIMESTAMP},
			</if>
		</set>
	</sql>

	<insert id="save" keyProperty="id" useGeneratedKeys="true" parameterType="com.lxzl.erp.dataaccess.domain.userLoginLog.UserLoginLogDO">
		INSERT INTO erp_user_login_log <include refid="set_column_sql"/>
	</insert>

	<update id="update" parameterType="com.lxzl.erp.dataaccess.domain.userLoginLog.UserLoginLogDO">
		UPDATE erp_user_login_log <include refid="set_column_sql"/> WHERE id = #{id, jdbcType=INTEGER}
	</update>
</mapper>