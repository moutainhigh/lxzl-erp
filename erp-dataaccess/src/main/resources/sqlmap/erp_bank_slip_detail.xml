	<select id="findBankSlipDetailDOCountByParams" resultType="Integer" parameterType="map">
		SELECT COUNT(ebsd.id)  FROM erp_bank_slip_detail ebsd
		INNER JOIN erp_bank_slip ebs ON ebsd.bank_slip_id = ebs.id AND ebs.data_status = 1
		<where>
			<if test="maps.bankSlipDetailQueryParam != null &amp;&amp; maps.bankSlipDetailQueryParam.payerName != null &amp;&amp; maps.bankSlipDetailQueryParam.payerName != ''">
				and ebsd.payer_name like CONCAT('%','${maps.bankSlipDetailQueryParam.payerName}','%' )
			</if>
			<if test="maps.bankSlipDetailQueryParam != null &amp;&amp; maps.bankSlipDetailQueryParam.loanSign != null ">
				and ebsd.loan_sign = #{maps.bankSlipDetailQueryParam.loanSign,jdbcType=INTEGER}
			</if>
			<if test="maps.bankSlipDetailQueryParam != null &amp;&amp; maps.bankSlipDetailQueryParam.detailStatus != null">
				and ebsd.detail_status = #{maps.bankSlipDetailQueryParam.detailStatus,jdbcType=INTEGER}
			</if>
			<if test="maps.bankSlipDetailQueryParam != null &amp;&amp; maps.bankSlipDetailQueryParam.bankSlipId != null ">
				and ebsd.bank_slip_id = #{maps.bankSlipDetailQueryParam.bankSlipId,jdbcType=INTEGER}
			</if>
			<if test="maps.isQueryAll == null">
				and ebsd.data_status = 1
			</if>
			<if  test="maps.departmentType != null &amp;&amp;maps.departmentType == 0">
				and 1=2
			</if>
			<if  test="maps.departmentType != null &amp;&amp;maps.departmentType == 1">
				and 1=1
			</if>
			<if test="maps.departmentType != null &amp;&amp;maps.departmentType == 2">
				and ebs.slip_status !=0
			</if>
		</where>
	</select>

