			</if>
			<if test="maps.couponQueryParam != null &amp;&amp; maps.couponQueryParam.customerNo != null &amp;&amp; maps.couponQueryParam.customerNo != ''">
				and ec.customer_no = LIKE CONCAT('%','${maps.couponQueryParam.customerNo}','%' )
			</if>
			<if test="true">
