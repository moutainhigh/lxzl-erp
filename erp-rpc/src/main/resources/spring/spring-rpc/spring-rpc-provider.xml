<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	     http://code.alibabatech.com/schema/dubbo
	     http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    
    <bean id="demoRpcService" class="com.lxzl.se.rpc.dubbo.provider.DubboService">
    	<property name="interfaceName" value="com.lxzl.erp.rpc.api.service.DemoRpcService" />
    	<property name="interfaceRef" ref="demoRpcServiceImpl" />
    	<property name="protocols" value="${dubbo.protocols}" />
    	<property name="owner" value="lxzl" />
    </bean>
    
    <bean id="testRpcService" class="com.lxzl.se.rpc.dubbo.provider.DubboService">
    	<property name="interfaceName" value="com.lxzl.erp.rpc.api.service.TestRpcService" />
    	<property name="interfaceRef" ref="testRpcServiceImpl" />
    	<property name="protocols" value="${dubbo.protocols}" />
    	<property name="owner" value="lxzl" />
    </bean>
     
    <bean id="userRpcService" class="com.lxzl.se.rpc.dubbo.provider.DubboService">
    	<property name="interfaceName" value="com.lxzl.erp.rpc.api.service.UserRpcService" />
    	<property name="interfaceRef" ref="userRpcServiceImpl" />
    	<property name="protocols" value="${dubbo.protocols}" />
    	<property name="owner" value="lxzl" />
    </bean>
    
	<bean id="dynamicDubboProvider" class="com.lxzl.se.rpc.dubbo.source.DynamicDubboProvider">
		<property name="openLog" value="${logback.dubbo.openLog}" />
	</bean>
	
	<bean id="dynamicCreateDubboProviderManager" class="com.lxzl.se.rpc.dubbo.source.DynamicCreateDubboProviderManager">
		<property name="dynamicDubboProvider" ref="dynamicDubboProvider" />
	</bean>
	  
</beans>
